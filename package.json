{
  "name": "productionist-js",
  "version": "0.0.1",
  "description": "A node.js port of the productionist library",
  "main": "index.js",
  "repository": "https://github.com/relimited/productionist.git",
  "author": "Johnathan Pagnutti",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "babel-node src",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack --progress",
    "lint": "eslint src webpack.config.babel.js --ext .js",
    "test": "yarn lint && jest --coverage",
    "prepush": "yarn test && yarn prod:build",
    "precommit": "yarn test"
  },
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "cross-env": "^5.0.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "commander": "^2.9.0",
    "fs": "^0.0.1-security",
    "regex": "^0.1.1"
  }
}
